<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="tetris" />
		<Option makefile="C:/sgdk/makefile.gen" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="sega_genesis_compiler" />
		<MakeCommands>
			<Build command="$make -f $makefile" />
			<CompileFile command="$make -f $makefile" />
			<Clean command="$make -f $makefile clean" />
			<DistClean command="$make -f $makefile distclean$target" />
			<AskRebuildNeeded command="$make -q -f $makefile $target" />
			<SilentBuild command="$make -f $makefile &gt; $(CMD_NULL)" />
		</MakeCommands>
		<Build>
			<Target title="default">
				<Option output="out/tetris" prefix_auto="1" extension_auto="1" />
				<Option object_output="out/" />
				<Option type="1" />
				<Option compiler="sega_genesis_compiler" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="tetris.h" />
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
